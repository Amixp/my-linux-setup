[Resolve]
# Primary DNS
DNS=192.168.1.202

# Fallback DNS
FallbackDNS=9.9.9.9

# Domains to resolve through specific servers
Domains=.docker

# Use DNS-over-HTTPS for fallback (Quad9 DoH)
DNSOverHTTPS=yes
DNSOverHTTPSProviders=dns.quad9.net

# Enable additional systemd-resolved features
MulticastDNS=no
DNSSEC=yes
