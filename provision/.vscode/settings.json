{
  "files.watcherExclude": {
    "**/.git/objects/**": true,
    "**/.git/subtree-cache/**": true,
    "**/node_modules/*/**": true,
    "**/.hg/store/**": true,
    ".flatpak/**": true,
    "_build/**": true
  },
  "python.formatting.provider": "autopep8",
  "editor.fontFamily": "'Droid Sans Mono', 'monospace', monospace"
}
